# Skript feito por Kick Holse
# Discord: https://www.kskripts.tk/discrd

options:
  time: 30 seconds
  tries: 3
  length:
    maximum: 32
    minimum: 5
messages:
  login:
    success: '\n &aVocê foi logado com sucesso, se divirta com nossos minigames.\n '
    wrong: '&cA senha que você inseriu esta incorreta, você tem mais {tries} tentativas!'
    usage: '&cUtilize /login <senha>'
    title:
      title: '&a&lREDE KS'
      subtitle: '&7Utilize /login <senha>'
    kicks:
      time-out: '&c&lREDE KS\n\n&c                    Você excedeu o tempo limite para se autenticar!'
      max-tries: '&c&lREDE KS\n\n&c                    Você excedeu o seu limite de tentativas!'
  register:
    different: '&cAs senhas que você inseriu não se coincidem!'
    your-nick: '&cA senha não pode ser seu nick!'
    success: '\n &aSua conta foi registrada com sucesso, se divirta com nossos minigames.\n '
    length: '&cA senha deve conter no mínimo {minimum} e no maximo {maximum} caracteres!'
    usage: '&cUtilize /register <senha> <senha>'
    title:
      title: '&a&lREDE KS'
      subtitle: '&7Utilize /register <senha> <senha>'
    kicks:
      time-out: '&c&lREDE KS\n\n&c                    Você excedeu o tempo limite para se autenticar!'
  changepassword:
    your-nick: '&cSua nova senha não pode ser seu nick!'
    success: '\n &aSua senha foi alterada com sucesso!\n '
    length: '&cA sua nova senha deve conter no mínimo {minimum} e no maximo {maximum} caracteres!'
    equal: '&cA sua senha nova não pode ser a mesma que a antiga!'
    wrong: '&cA senha antiga que você inseriu esta incorreta!'
    usage: '&cUtilize /changepassword <senha-antiga> <senha-nova>'
